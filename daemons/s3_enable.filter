.Statement |= .+ [
  {
    "Effect": "Allow",
    "Action": [
      "s3:AbortMultipartUpload",
      "s3:Get*",
      "s3:List*",
      "s3:PutObject*"
    ],
    "Resource": [
      "arn:aws:s3:::$DSS_S3_BUCKET",
      "arn:aws:s3:::$DSS_S3_BUCKET/*",
      "arn:aws:s3:::$DSS_S3_BUCKET_TEST",
      "arn:aws:s3:::$DSS_S3_BUCKET_TEST/*"
    ]
  },
  {
    "Effect": "Allow",
    "Action": [
      "s3:Get*",
      "s3:List*"
    ],
    "Resource": "*"
  }
]
